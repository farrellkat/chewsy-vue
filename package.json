{
  "name": "chewsy",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "view": "vue-helper-view",
    "component": "vue-helper-component",
    "filter": "vue-helper-filter",
    "module": "vue-helper-module",
    "model": "vue-helper-model",
    "service": "vue-helper-service",
    "mixin": "vue-helper-mixin",
    "gen-svgs": "node ./svgs/svgs.component.js"
  },
  "dependencies": {
    "@escofieldpublic/vue-file-helper": "^0.2.5",
    "animate.css": "^3.7.2",
    "axios": "^0.21.1",
    "core-js": "^3.6.4",
    "html2pug": "^4.0.0",
    "postcss": "^7.0.36",
    "postcss-preset-env": "^6.7.0",
    "pug": "^2.0.4",
    "pug-plain-loader": "^1.0.0",
    "v-show-slide": "^1.8.3",
    "vue": "^2.6.11",
    "vue-axios": "^2.1.5",
    "vue-router": "^3.1.6",
    "vue-tidyroutes": "^0.1.2",
    "vue2-touch-events": "^2.2.1",
    "vuex": "^3.1.3"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.3.0",
    "@vue/cli-plugin-eslint": "~4.3.0",
    "@vue/cli-plugin-router": "~4.3.0",
    "@vue/cli-plugin-vuex": "~4.3.0",
    "@vue/cli-service": "~4.3.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-vue": "^6.2.2",
    "prettier": "^1.19.1",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/prettier"
    ],
    "rules": {
      "semi": 0,
      "no-console": "off",
      "no-unused-vars": [
        "error",
        {
          "argsIgnorePattern": "^_",
          "varsIgnorePattern": "^_"
        }
      ]
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all",
    "printWidth": 120
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {},
      "postcss-preset-env": {
        "features": {
          "nesting-rules": true,
          "custom-media-queries": true,
          "color-mod-function": true
        }
      }
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
